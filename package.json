  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "builtin-modules": "3.3.0",
    "esbuild": "0.15.7",
    "eslint": "^8.23.0",
    "handlebars": "^4.7.7",
    "obsidian": "latest",
    "prettier": "^2.7.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {}
}
