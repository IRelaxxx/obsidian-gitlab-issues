{
  "name": "obsidian-gitlab-issues",
  "version": "1.0.1",
  "description": "Obsidian plugin to import issues from GitLab.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
